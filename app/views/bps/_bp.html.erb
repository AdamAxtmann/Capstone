<% unless bp.deleted %>
	<li>
	  <span class="date"><%= link_to bp.date.strftime('%B %-d, %Y'), [bp.phr, bp] %></span><br/>
	  <span class="timestamp">
	    Last updated <%= time_ago_in_words(bp.updated_at) %> ago.
	    <%= link_to "Delete", [bp.phr, bp], method: :delete, data: { confirm: "Are you sure you want to delete this Blood Pressure Record?" } , title: bp.date, class: "btn btn-small btn-secondary" %>
	  </span>
	</li>
<% end %>