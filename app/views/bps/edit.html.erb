<% provide(:title, 'Edit Blood Pressure Record') %>

<%= form_for([@phr, @bp]) do |f| %>
<%= render 'shared/phr_info' %>
  <div class="row">
      <h2>Update Blood Pressure Record</h2>
      <hr>
      <%= render 'shared/error_messages', object: f.object %>
     
        <div class="span6">
          <%= render 'bp_form', f: f %>
        
          <%= f.submit "Save Changes", class: "btn btn-small btn-secondary" %>
          <%= link_to 'Cancel', phr_bps_path, class: "btn btn-small btn-secondary" %>
        </div>


  </div>
<% end %>
</div>