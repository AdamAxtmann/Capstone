<% unless eye.deleted %>
	<tr>
		<td><%= link_to eye.date.strftime('%B %-d, %Y'), [eye.phr, eye]  %></td>
		<td><%= eye.doctor %></td>
		<td><%= link_to "Edit", edit_phr_eye_path(@phr, eye), class: "btn btn-small btn-secondary" %></td>
		
		<td><%= link_to 'Delete', [eye.phr, eye], method: :delete, data: {confirm: "Are you sure you want to delete this optometrist record?"}, title: eye.date.strftime('%B %-d, %Y'), class: "btn btn-small btn-secondary" %></td>
	</tr>
<% end %>