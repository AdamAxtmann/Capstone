<% provide(:title, 'New Optometrist Record') %>

<%= form_for([@phr, @eye]) do |f| %>
<%= render 'shared/phr_info' %>
  <div class="row">


      <h1>Create New Optometrist Record</h1>
      <%= render 'shared/error_messages', object: f.object %>
     
        <div class="span6">
          <%= render 'eye_form', f: f %>
        
          <%= f.submit "Create Record" %><br/>
          <%= link_to 'Cancel', phr_eyes_path %>
        </div>


  </div>  
<% end %>
</div>