<% provide(:title, 'New Optometrist Record') %>
<%= render 'shared/phr_info' %>
<section>
<%= form_for([@phr, @phr.eyes.build]) do |f| %>
  <div class="row">


      <h1>Create New Optometrist Record</h1>
      <%= render 'shared/error_messages', object: f.object %>
     
        <div class="span6">
          <%= render 'eye_form', f: f %>
        
          <%= f.submit "Create Record" %>
        </div>


  </div>
  <%= link_to 'Back', phr_eyes_path %>
<% end %>
</div>
</section>