<%= provide(:title, 'New Ailment Record') %>
<div class ="row">
	<%= render 'shared/phr_info' %>
	<section>
		<div class="span8">
			<h2 style = "text-align: left;">New Ailment Record</h2>
		<hr>

	<%= form_for([@phr, @ailment]) do |f| %>
		<%= render 'shared/error_messages', object: f.object %>

		<div class="span6">
			<%= render 'form', f: f %>
			<%= link_to 'Cancel', phr_ailments_path %>
		
		</div>
<% end %>
		



