<tr>
		<td><%= immunization.date.strftime('%B %-d, %Y') %></td>
		<td><%= link_to immunization.immunization, [immunization.phr, immunization] %></td>
		<td><%= immunization.expiry.strftime('%B %-d, %Y') %></td>
		<td><%= link_to "Edit", edit_phr_immunization_path(@phr, immunization), class: "btn btn-small btn-secondary" %></td>
		
		<td><%= link_to 'Delete', [immunization.phr, immunization], method: :delete, data: {confirm: "Are you sure you want to delete this immunization record?"}, title: immunization.immunization, class: "btn btn-small btn-secondary" %></td>
</tr>