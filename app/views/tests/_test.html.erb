<% unless test.deleted %>
	<tr>
		<td><%= test.date.strftime('%B %-d, %Y') %></td>
		<td><%= link_to test.name, [test.phr, test] %></td>
		<td><%= link_to "Edit", edit_phr_test_path(@phr, test), class: "btn btn-small btn-secondary" %></td>
		
		<td><%= link_to 'Delete', [test.phr, test], method: :delete, data: {confirm: "Are you sure you want to delete this test record?"}, title: test.name, class: "btn btn-small btn-secondary" %></td>
	</tr>
<% end %>