<% provide(:title, 'Edit Test Results') %>


<div class ='row'>
  <%= render 'shared/phr_info' %>
    <div class="span8">
        <h2 style="text-align:left;">Update Test Results Record</h2>
        <hr>

        <%= form_for([@phr, @test]) do |f| %>
          <%= render 'shared/error_messages', object: f.object %>
         
            <div class="span6">
              <%= render 'test_form', f: f %>
            
              <%= f.submit "Save", class: "btn btn-small btn-secondary" %> <%= link_to 'Cancel', phr_tests_path, class: "btn btn-small btn-secondary" %>
            </div>
        <% end %>
    </div>
  
</div>