<% provide(:title, 'Edit Test Results') %>

<%= form_for([@phr, @test]) do |f| %>
<%= render 'shared/phr_info' %>
  <div class="row">
      <h2 style="text-align:left;">Update Test Results Record</h2>
      <hr>
      <%= render 'shared/error_messages', object: f.object %>
     
        <div class="span6">
          <%= render 'test_form', f: f %>
        
          <%= f.submit "Save Changes", class: "btn btn-small btn-secondary" %>
        </div>
  </div>
  <%= link_to 'Back', phr_test_path %>
<% end %>
</div>