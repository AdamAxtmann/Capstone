<% provide(:title, "Edit PHR") %>
<h1>Update your PHR</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@phr) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= render 'phr_form', f: f  %>
      
      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>